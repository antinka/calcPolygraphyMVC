@model IEnumerable<calculator.Models.CharacterPrintMachine>

@{
    ViewBag.Title = "Характеристика скорости печати";
}

<h2>Характеристика скорости печати</h2>
<table class="table">
    <tr>
        <th>
            Количество листов в минуту
        </th>
    
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QuantityListInMinute)
        </td>
  
        <td>
            @Html.ActionLink("Редактировать", "EditCharPrintMach", new { id = item.Id }) 
        </td>
    </tr>
}

</table>
