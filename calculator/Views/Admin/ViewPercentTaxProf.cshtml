@model IEnumerable<calculator.Models.PercenTaxProfit>

@{
    ViewBag.Title = "процент налога / прибыли";
}

<h2>Процент налога / прибыли</h2>


<table class="table">
    <tr>
        <th>
         Налог %
        </th>
        <th>
         Прибыль %
        </th> 
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tax)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Profit)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "EditPercentTaxProf", new { id = item.id }) 
          
    </tr>
}

</table>
